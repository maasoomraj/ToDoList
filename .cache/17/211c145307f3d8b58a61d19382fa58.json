ody {\n    /* background-color: #1f2833; */\n    background-color: #F8B469;\n    color: #fff;\n    text-align: center;\n    font-size: 20 px;\n    font-family: 'Baloo 2', cursive;\n    word-wrap: break-word;\n    /* align-items: center; */\n}\n\n.label , .already {\n    text-align: center;\n    font-size: 18px;\n    display: inline;\n    color: #0d0d0d;\n    margin-right: 5px;\n    margin-left: 20px;\n}\n\n.App {\n    display: flex;\n    flex-direction: column;\n    /* align-items: center; */\n}\n\n.Block , .AddTask {\n    border: 1px solid #000000;\n    padding: 3%;\n    margin: 2%;\n    color : #050B13;\n}\n\n.AddTask{\n    background:  #91CCF4;\n}\n\n.inputBox {\n    border: 1px solid rgb(0, 0, 0);\n}\n\na, a:hover {\n    color: #000000;\n}\n\n.flashMessage{\n    color: darkblue;\n    font-size: 18px;\n    margin: 8px;\n}\n\n.button {\n    display: block;\n    width: 115px;\n    height: 40px;\n    background: #B4DFE5;\n    padding: 8px;\n    margin-top: 8px;\n    text-align: center;\n    border-radius: 10px;\n    color: #0d0d0d;\n    font-weight: bold;\n    font-size: medium;\n}\n\n.navbar , .navbar-mini {\n    width: 100%;\n    background-color: #FDEBAD;\n    overflow: auto;\n    /* margin-left: 12px; */\n  }\n\n  .task-button{\n    background-color: #FDEBAD;\n    float: right;\n    border: none;\n    display:block;\n  }\n\n  .navbar a , .logout, .username , .task-button ,.username-mini{\n    float: left;\n    padding: 15px;\n    color: #0d0d0d;\n    text-decoration: none;\n    font-size: 19px;\n  }\n\n  .logout:hover{\n    background-color: #c38d9e;\n  }\n\n  .username{\n      background-color: #F98989;\n  }\n\n  .username-mini{\n      background-color: #cad8fc;\n      padding-left: 4%;\n      padding-right: 4%;\n      text-decoration: underline;\n      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n      letter-spacing: 2px;\n  }\n\n  .navbar a:hover {\n    background-color: #c38d9e;\n  }\n\n  .navbar a.active {\n    background-color: #4CAF50;\n  }\n\n  .heading { \n    background-color: #e8a87c;\n  }\n\n.home{\n    font-size: 35px;\n    padding-top: 12%;\n    color: rgb(2, 33, 55);\n}\n\n.title{\n    font-size: 35px;\n    color: #0d0d0d;\n    padding: 1%;\n}\n\na:hover {\n    text-decoration: none;\n}\n\n.button-delete{\n    background-color: #d52e2e;\n    height: 40px;\n    width: 100px;\n    float: right;\n    margin: auto;\n}\n\n.button-delete:hover{\n    background-color: #b30d0d;\n    box-shadow: 0px 20px 20px 0px rgba(0, 0, 0, 0.6);\n}\n\n.button-vote, .button-view, .button-delete, .button-work , .button-comp{\n    border: none;\n    color: white;\n    text-align: center;\n    text-decoration: none;\n    display: inline-block;\n    font-size: 16px;\n    margin: 4px 2px;\n    margin-left: 20px;\n    cursor: pointer;\n    margin-bottom: 10px;\n}\n\n.button-vote{\n    background-color: rgb(45, 86, 136);\n    padding: 10px;\n    height: 45px;\n    width: 120px;\n}\n\n.button-view{\n    background-color: rgb(45, 86, 136);\n    height: 40px;\n    width: 100px;\n}\n\n.button-vote:hover, .button-view:hover{\n    box-shadow: 0px 20px 20px 0px rgba(0, 0, 0, 0.6);\n    background-color:rgb(187, 34, 23);\n}\n\n.dropdown {\n    color : #0d0d00;\n    height : 40px;\n    width: 140px;\n}\n\n.task{\n    box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);\n    transition: 0.3s;\n    width: 90%;\n    margin: 50px;\n}\n\n.task:hover{\n  box-shadow: 0px 20px 20px 0px rgba(0, 0, 0, 0.6);\n}\n\n/* .taskNameNew, .taskNameIn, .taskNameComp , .taskMenu{\n  font-size: 20px;\n  font-family: monospace;\n  padding: 20px;\n  text-align: center;\n  color: #000000;\n  height: 60px;\n} */\n\n.taskNameNew{\n    background-color: #44d52e;\n}\n\n.taskNameIn , .button-work{\n    background-color: #d5c72e;\n}\n\n.taskNameComp , .button-comp{\n    background-color: #d52e2e;\n}\n\n.taskDetails , .taskDetailsComments{\n  padding-top: 10px;\n  padding-bottom: 10px;\n  font-size: 18px;\n  font-family: initial;\n}\n\n.taskDetails{\n  text-align: center;\n  color: #01061a;\n  background-color: #cad8fc;\n}\n\n.taskDetailsComments{\n    background-color: #c38d9e;\n    color: #0d0d0d;\n}\n\n.calendar {\n    height : 40px;\n    width: 120 px;\n}\n\n.home-line , .button-update{\n    display: inline;\n    margin: 10px;\n}\n\n.button-update:hover{\n    box-shadow: 0px 20px 20px 0px rgba(0, 0, 0, 0.6);\n    background-color:rgb(187, 34, 23);\n}"},"lineCount":246}},"error":null,"hash":"cee1d12fa8c4b0b22ec81c4b97f8735f","cacheData":{"env":{}}}